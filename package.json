{
  "name": "kisstastic",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "size": "size-limit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@size-limit/file": "^7.0.3",
    "@types/pixi.js": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "astro": "^0.21.12",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.3",
    "postcss": "^8.4.1",
    "prettier": "^2.4.1",
    "prettier-plugin-astro": "0.0.11",
    "size-limit": "^7.0.3",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{html,css}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist",
      "limit": "100 kB"
    }
  ],
  "dependencies": {
    "pixi.js": "^6.2.2"
  }
}
